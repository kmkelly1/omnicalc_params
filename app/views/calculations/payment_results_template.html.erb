<h1>Payment Results</h1>
<dl>
    <dt>APR</dt>
        <dd> <%= number_to_percentage(@interest_rate, precision: 4) %> %</dd>
    <dt> Number of years</dt>
        <dd> <%= @years %> </dd>
    <dt> Principal</dt>
        <dd> <%= number_to_currency(@principal, precision:2) %> </dd>
    <dt>Payment </dt> 
        <dd> <%= number_to_currency(@monthly_payment_2, precision: 2) %> </dd>
</dl>
